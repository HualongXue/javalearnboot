<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true" scanPeriod="60 seconds" debug="false">
 <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
  <encoder>
   <!-- level time thread class method message newLine -->
   <pattern>%p %d{yyyy-MM-dd HH:mm:ss.SSS} %t %C %M : %msg%n</pattern>
   <!-- charset>UTF-8</charset -->
  </encoder>
 </appender>
 <logger name="cn.huimin.learnboot" level="DEBUG"></logger>
 <root level="info">
  <!--控制台输出-->
  <appender-ref ref="STDOUT"/>
  
 </root>
</configuration>